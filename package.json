{
  "name": "blueprints",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "node scripts/save-commit-info.script && nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "node scripts/save-commit-info.script && nest start --watch",
    "start:debug": "node scripts/save-commit-info.script && nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:grpc": "set NODE_ENV=test&& node dist/apps/app-grpc/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/blueprints/test/jest-e2e.json",
    "serve:docs": "./node_modules/.bin/compodoc -p tsconfig.json -s -r $PORT",
    "test:e2e:obj-factory-v1": "jest --config ./apps/obj-factory-v1/test/jest-e2e.json",
    "test:e2e:obj-factory-v1-typeorm": "jest --config ./apps/obj-factory-v1-typeorm/test/jest-e2e.json",
    "test:e2e:obj-factory-v1-authn": "jest --config ./apps/obj-factory-v1-authn/test/jest-e2e.json",
    "build-schematics": "./node_modules/.bin/tsc -p ./schematics/tsconfig.json",
    "build-schematics:dev": "./node_modules/.bin/tsc -p ./schematics/tsconfig.json --watch"
  },
  "dependencies": {
    "@nestjs/common": "^8.0.4",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.4",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "^8.0.1",
    "@nestjs/platform-express": "^8.0.4",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.0.9",
    "@nestjs/typeorm": "^8.0.2",
    "@prisma/client": "^2.30.3",
    "@types/passport-azure-ad": "^4.3.1",
    "ajv": "^8.6.2",
    "ajv-formats": "^2.1.1",
    "ajv-merge-patch": "^5.0.1",
    "aws-sdk": "^2.936.0",
    "axios": "^0.21.4",
    "axios-concurrency": "^1.0.4",
    "class-transformer": "0.4.0",
    "class-validator": "^0.13.1",
    "file-type": "^16.5.3",
    "grpc": "^1.24.10",
    "jwks-rsa": "^2.0.5",
    "lodash": "^4.17.21",
    "mime": "^2.5.2",
    "passport": "^0.5.0",
    "passport-azure-ad": "^4.3.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.37",
    "uuid": "^8.3.2",
    "xlsx": "^0.17.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.11",
    "@grpc/grpc-js": "^1.3.5",
    "@grpc/proto-loader": "^0.6.4",
    "@nestjs/cli": "^8.0.2",
    "@nestjs/microservices": "^7.6.15",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.4",
    "@types/express": "^4.17.13",
    "@types/jest": "^24.0.18",
    "@types/mime": "^2.0.3",
    "@types/multer": "^1.4.7",
    "@types/node": "^12.20.17",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/pg": "^8.6.1",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.1",
    "@types/validator": "^10.11.3",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^4.28.4",
    "@typescript-eslint/parser": "^4.28.4",
    "axios-mock-adapter": "^1.20.0",
    "clean-webpack-plugin": "^3.0.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.31.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "jest-junit": "^12.2.0",
    "json-schema-to-typescript": "^10.1.4",
    "prettier": "^2.3.2",
    "prisma": "^2.30.3",
    "supertest": "^6.1.3",
    "ts-jest": "^26.4.4",
    "ts-loader": "^6.1.1",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  }
}
